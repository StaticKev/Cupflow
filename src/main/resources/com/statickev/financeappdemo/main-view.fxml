<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.statickev.financeappdemo.MainPaneController">
    <Pane fx:id="navigationDrawer" layoutX="53.0" prefHeight="600.0" prefWidth="140.0" style="-fx-background-color: #3b3b3b;">
        <Label fx:id="dashboardButton" layoutY="167.0" prefHeight="53.0" prefWidth="140.0"
               style="-fx-background-color: #3b3b3b;" text="  Dashboard" textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0"/>
            </font>
        </Label>
        <Label fx:id="profileButton" layoutY="220.0" prefHeight="53.0" prefWidth="140.0" text="  Profile"
               textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0"/>
            </font>
        </Label>
        <Label fx:id="incomeButton" layoutY="273.0" prefHeight="53.0" prefWidth="140.0" text="  My Income"
               textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0"/>
            </font>
        </Label>
        <Label fx:id="expenseButton" layoutY="326.0" prefHeight="53.0" prefWidth="140.0" text="  My Expenses"
               textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0"/>
            </font>
        </Label>
        <Label fx:id="analyticsButton" layoutY="379.0" prefHeight="53.0" prefWidth="140.0" text="  Analytics"
               textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0"/>
            </font>
        </Label>
    </Pane>
    <Pane fx:id="navigationDrawerIcons" prefHeight="600.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
        <Pane layoutY="167.0" prefHeight="53.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
            <ImageView fx:id="dashboardButtonIcon" fitHeight="20.0" fitWidth="20.0" layoutX="18.0" layoutY="16.0"
                       pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </Pane>
        <Pane layoutY="220.0" prefHeight="53.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
            <ImageView fx:id="profileButtonIcon" fitHeight="20.0" fitWidth="20.0" layoutX="18.0" layoutY="16.0"
                       pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </Pane>
        <Pane layoutY="273.0" prefHeight="53.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
            <ImageView fx:id="incomeButtonIcon" fitHeight="20.0" fitWidth="20.0" layoutX="18.0"
                       layoutY="16.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </Pane>
        <Pane layoutY="326.0" prefHeight="53.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
            <ImageView fx:id="expenseButtonIcon" fitHeight="20.0" fitWidth="20.0" layoutX="18.0"
                       layoutY="16.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </Pane>
        <Pane layoutY="379.0" prefHeight="53.0" prefWidth="53.0" style="-fx-background-color: #3b3b3b;">
            <ImageView fx:id="analyticsButtonIcon" fitHeight="20.0" fitWidth="20.0" layoutX="18.0"
                       layoutY="16.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </Pane>
    </Pane>
</AnchorPane>
